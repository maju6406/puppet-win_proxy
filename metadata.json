{
    "name": "beersy-win_proxy",
    "version": "0.2.0",
    "author": "beersy",
    "summary": "A puppet module to manage the proxy settings within Window's Internet Settings snap-in",
    "license": "Apache 2.0",
    "source": "https://github.com/maju6406/win_proxy",
    "project_page": "https://github.com/maju6406/win_proxy",
    "issues_url": "https://github.com/maju6406/win_proxy/issues",
    "dependencies": [{
            "name": "puppetlabs-stdlib",
